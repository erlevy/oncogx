if (lymph <= lymph_med) {lymphocyte_presence <- c(lymphocyte_presence, "0")}
else {lymphocyte_presence <- c(lymphocyte_presence, "1")}
}
tnbc$lymphocyte_presence <- as.numeric(lymphocyte_presence)
